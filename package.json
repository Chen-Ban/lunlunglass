{
  "name": "blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"electron ./desktop/main.js\"",
    "build": "tsc && vite build && concurrently \"serve ./dist\" \"electron ./desktop/main.js\"",
    "lint": "eslint . --ext ts,tsx,cjs --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier . --write",
    "prepare": "husky",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "initDB": "node ./desktop/initCustomer.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/pinyin": "^2.10.2",
    "ahooks": "^3.8.0",
    "antd": "^5.18.0",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "pinyin": "^4.0.0-alpha.2",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-transition-group": "^4.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@craco/craco": "^7.1.0",
    "@types/antd": "^1.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/mockjs": "^1.0.10",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-transition-group": "^4.4.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "classname": "^0.0.0",
    "concurrently": "^8.2.2",
    "electron": "^31.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "mockjs": "^1.1.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.1",
    "sass": "^1.77.4",
    "ts-jest": "^29.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
